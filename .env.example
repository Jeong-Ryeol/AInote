# Database (Docker 환경에서는 host.docker.internal 사용)
DATABASE_URL="postgresql://postgres:password@host.docker.internal:5432/ainote"

# Redis
REDIS_URL="redis://host.docker.internal:6379"

# NextAuth
AUTH_SECRET="generate-with-openssl-rand-base64-32"

# API 키 암호화 키 (AES-256, 64자 hex)
ENCRYPTION_KEY="generate-with-openssl-rand-hex-32"

# 협업 서버 JWT 시크릿
COLLAB_JWT_SECRET="generate-with-openssl-rand-base64-32"
